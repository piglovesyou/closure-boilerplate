{
  "id": "plovr-sample",

  "paths": [
	  "./public/app",
	  "./public/javascripts"
  ],
  "inputs": "public/javascripts/main.js",
  "mode": "ADVANCED",
  "level": "VERBOSE",
  "output-file": "public/javascripts/main-min.js",

  "css-inputs": "public/stylesheets/main.gss",
  "css-output-file": "public/stylesheets/main-min.css",

  "modules": {
    "main": {
      "inputs": "public/javascripts/main.js",
      "deps": [] // This must be the root module.
    },
    "header": {
      "inputs": [
        "public/javascripts/header.js"
      ],
      "deps": "main"
    }
  }

}
